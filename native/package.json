{
  "name": "my-expo-app",
  "version": "1.0.0",
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "start": "expo start",
    "prebuild": "expo prebuild",
    "apk": "./build-apk.sh",
    "diagnose": "./diagnose-build.sh",
    "clean": "rm -rf node_modules/.cache .expo android/app/build android/build",
    "clean:full": "rm -rf node_modules/.cache .expo android/app/build android/build android/.gradle && cd android && ./gradlew clean && cd ..",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web"
  },
  "dependencies": {
    "@expo-google-fonts/bangers": "^0.4.1",
    "@expo-google-fonts/inter": "^0.4.2",
    "@expo-google-fonts/open-sans": "^0.4.2",
    "@gorhom/bottom-sheet": "^5.2.8",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@reduxjs/toolkit": "^2.11.2",
    "expo": "^54.0.0",
    "expo-blur": "^15.0.8",
    "expo-constants": "^18.0.13",
    "expo-dev-client": "^6.0.20",
    "expo-device": "^8.0.10",
    "expo-font": "^14.0.10",
    "expo-image-picker": "~17.0.10",
    "expo-location": "^19.0.8",
    "expo-notifications": "^0.32.16",
    "expo-status-bar": "~3.0.8",
    "nativewind": "latest",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-native": "0.81.5",
    "react-native-maps": "1.20.1",
    "react-native-razorpay": "^2.3.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-render-html": "^6.3.4",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-svg": "^15.15.1",
    "react-native-worklets": "0.5.1",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~19.1.10",
    "eslint": "^9.25.1",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.2",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.9.2"
  },
  "main": "node_modules/expo/AppEntry.js",
  "private": true
}
